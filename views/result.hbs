<div class="alert alert-danger">
  共輸入{{dataVol}}筆IP，無效IP數: {{invalidVol}}
</div>

<table class="table">
  <thead>
    <tr>
      <th scope="col">IP</th>
      <th scope="col">Region</th>
      <th scope="col">Country</th>
      <th scope="col">City</th>
    </tr>
  </thead>
  <tbody>
    {{#each ipLocation}}
    <tr>
      <th scope="row">{{this.IP}}</th>
      <td>{{this.region}}</td>
      <td>{{this.country}}</td>
      <td>{{this.city}}</td>
    </tr>
    {{/each}}
  </tbody>
</table>